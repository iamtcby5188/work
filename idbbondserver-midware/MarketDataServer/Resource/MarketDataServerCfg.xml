        <ExtensionQuantum>4096</ExtensionQuantum>
        <MonitorInterval>60</MonitorInterval>
        <InitData>true</InitData>
        <LogFile>FastDB.log</LogFile>
    </FastDB>
    <LogMsg>
        <Enable>True</Enable>
        <Destination>File</Destination>
    </LogMsg>
    <CommonServerCfgFilePath/>
