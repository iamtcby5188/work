<?xml version="1.0" encoding="utf-8"?>

<Config>
    <Connection>
        <Host>tcp://127.0.0.1:3306</Host>
        <User>artogrid</User>
        <Password>artogrid</Password>
        <DataBase/>
        <PoolSize>5</PoolSize>
    </Connection>
    <SDBus>
        <Main>
            <Url>127.0.0.1</Url>
			<HeartBeat>-1</HeartBeat>
        </Main>
    </SDBus>
    <Queue>
        <Main>Queue.Bond.MarketData.1</Main>
    </Queue>
    <FastDB>
        <Host>localhost</Host>
